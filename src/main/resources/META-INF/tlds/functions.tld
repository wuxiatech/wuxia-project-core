    <function>
        <name>simpleParse</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String stringSimpleParse(java.lang.String,java.lang.Object)
        </function-signature>
    </function>
    <function>
        <name>spelParse</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String stringSPELParse(java.lang.String,java.lang.Object)
        </function-signature>
    </function>
    <function>
        <name>menus</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>java.util.List menus(java.lang.String)</function-signature>
    </function>

    <function>
        <name>ipaddr</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>java.lang.String ipBelongingTo(java.lang.String)</function-signature>
