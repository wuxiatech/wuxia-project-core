        <function-signature>
            java.lang.String replace(java.lang.String,java.lang.String,java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>replaceic</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String replaceic(java.lang.String,java.lang.String,java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>replaceBetween</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String replaceBetween(java.lang.String,java.lang.Integer,java.lang.Integer)
        </function-signature>
    </function>
    <function>
        <name>contants</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            boolean contants(java.lang.Object,java.lang.Object)
        </function-signature>
    </function>
    <function>
        <name>jsonToMap</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.util.Map jsonToMap(java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>toJson</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String toJson(java.lang.Object)
        </function-signature>
    </function>

    <function>
        <description>根据字典id获取字典对象，返回的是DicBean对象</description>
        <name>dic</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            cn.wuxia.project.basic.core.conf.support.DicBean getDicById(java.lang.String)
        </function-signature>
        <example>${ifn:dic(dicid)}</example>
    </function>
    <function>
        <description>根据字典code获取字典值</description>
        <name>dicbycode</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String getDic(java.lang.String, java.lang.String)
        </function-signature>
        <example>${ifn:dicbycode(diccode, dicparentcode)}</example>
    </function>
    <function>
        <description>根据字典code获取字典值</description>
        <name>dic2</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String getDicByCode(java.lang.String)
        </function-signature>
        <example>${ifn:dic2(diccode)}</example>
    </function>
    <function>
        <name>dics</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.util.List getDicsByParentCode(java.lang.String)
        </function-signature>
    </function>

    <function>
        <name>tag</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            cn.wuxia.project.basic.core.conf.support.TagBean getTag(java.lang.String)
        </function-signature>
        <example>${ifn:tag(tagid)}</example>
    </function>

    <function>
        <name>tags</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.util.List getTags(java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>ctx</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
