<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        version="2.0">
    <tlib-version>1.0</tlib-version>
    <jsp-version>2.0</jsp-version>
    <short-name>core functions</short-name>
    <uri>/project/functions</uri>
    <display-name>content management system functions lib</display-name>
    <description>Tag lib for functions used in jsp.</description>
    <function>
        <name>encodeUrl</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String encodeUrl(java.lang.Object)
        </function-signature>
    </function>
    <function>
        <name>decodeUrl</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String decodeUrl(java.lang.Object)
        </function-signature>
    </function>
    <function>
        <name>encode</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String encode(java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>decode</name>
        <function-class>cn.wuxia.project.basic.support.Functions</function-class>
        <function-signature>
            java.lang.String decode(java.lang.String)
        </function-signature>
    </function>
    <function>
        <name>replace</name>
